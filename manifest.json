{
  "name": "FiltrFeed",
  "version": "0.4.0",
  "description": "A way to direct you to the good",
  "permissions": ["storage", "tabs", "activeTab", "scripting", "storage"],
  "host_permissions": ["https://*.youtube.com/*"],
  "background":{
    "service_worker" : "service_worker.js"
  },
  "content_scripts": [
  {
    "matches": ["https://*.youtube.com/*"],
    "js": ["workIt.js"],
    "type": "module"
  }
],
  "action":{
    "default_icon": "icon.png",
    "default_title": "FiltrFeed",
    "default_popup": "index.html"
  },
  "manifest_version" : 3
}